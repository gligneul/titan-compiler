function sieve(N: integer): {integer}

    local is_prime: {boolean} = {}
    is_prime[1] = false
    for n:integer = 2, N do
        is_prime[n] = true
    end

    local nprimes: integer = 0
    local primes: {integer} = {}

    for n:integer = 1, N do
        if is_prime[n] then
            nprimes = nprimes + 1;
            primes[nprimes] = n
            for m:integer = n+n, N, n do
                is_prime[m] = false
            end
        end
    end

    return primes
end
